package Me.Akio.ChaosAwakens.Models;

/*     */ import net.minecraft.client.model.ModelBase;
import net.minecraft.client.model.ModelBox;
/*     */ import net.minecraft.client.model.ModelRenderer;
/*     */ import net.minecraft.entity.Entity;
/*     */ import net.minecraft.util.MathHelper;
import net.minecraft.world.World;
/*     */ 
public class ModelBeaver extends ModelBase {
	private final ModelRenderer body;
	private final ModelRenderer tail;
	private final ModelRenderer tail_r1;
	private final ModelRenderer head;
	private final ModelRenderer rightfrontleg;
	private final ModelRenderer leftfrontleg;
	private final ModelRenderer rightbackleg;
	private final ModelRenderer leftbackleg;

	public ModelBeaver() {
		textureWidth = 64;
		textureHeight = 32;

		body = new ModelRenderer(this);
		body.setRotationPoint(0.0F, 24.0F, 0.0F);
		body.cubeList.add(new ModelBox(body, 0, 0, -4.0F, -9.0F, -4.0F, 8, 7, 10, 0.0F));
		tail = new ModelRenderer(this);
		tail.setRotationPoint(0.0F, 21.0F, 5.0F);
		this.tail.mirror = true;
		this.tail_r1.mirror = true;
		this.head.mirror = true;
		this.rightbackleg.mirror = true;
		this.rightfrontleg.mirror = true;
		this.leftbackleg.mirror = true;
		this.leftfrontleg.mirror = true;

		tail_r1 = new ModelRenderer(this);
		tail_r1.setRotationPoint(0.0F, 3.0F, -5.0F);
		tail.addChild(tail_r1);
		setRotationAngle(tail_r1, 0.1309F, 0.0F, 0.0F);
		tail_r1.cubeList.add(new ModelBox(tail_r1, 0, 17, -3.0F, -3.0F, 6.0F, 6, 1, 8, 0.0F));

		head = new ModelRenderer(this);
		head.setRotationPoint(0.0F, 19.0F, -4.0F);
		head.cubeList.add(new ModelBox(head, 23, 21, -2.0F, -3.0F, -5.0F, 4, 4, 5, 0.0F));
		head.cubeList.add(new ModelBox(head, 20, 23, -1.0F, 1.0F, -5.0F, 2, 1, 1, 0.0F));

		rightfrontleg = new ModelRenderer(this);
		rightfrontleg.setRotationPoint(-2.0F, 22.0F, 0.0F);
		rightfrontleg.cubeList.add(new ModelBox(rightfrontleg, 0, 0, -1.0F, 0.0F, -3.0F, 2, 2, 2, 0.0F));
		rightfrontleg.cubeList.add(new ModelBox(rightfrontleg, 0, 21, -1.0F, 1.0F, -4.0F, 2, 1, 1, 0.0F));

		leftfrontleg = new ModelRenderer(this);
		leftfrontleg.setRotationPoint(2.0F, 22.0F, -2.0F);
		leftfrontleg.cubeList.add(new ModelBox(leftfrontleg, 0, 0, -1.0F, 0.0F, -1.0F, 2, 2, 2, 0.0F));
		leftfrontleg.cubeList.add(new ModelBox(leftfrontleg, 0, 21, -1.0F, 1.0F, -2.0F, 2, 1, 1, 0.0F));

		rightbackleg = new ModelRenderer(this);
		rightbackleg.setRotationPoint(-2.0F, 22.0F, 4.0F);
		rightbackleg.cubeList.add(new ModelBox(rightbackleg, 0, 0, -1.0F, 0.0F, -1.0F, 2, 2, 2, 0.0F));
		rightbackleg.cubeList.add(new ModelBox(rightbackleg, 0, 21, -1.0F, 1.0F, -2.0F, 2, 1, 1, 0.0F));

		leftbackleg = new ModelRenderer(this);
		leftbackleg.setRotationPoint(2.0F, 22.0F, 4.0F);
		leftbackleg.cubeList.add(new ModelBox(leftbackleg, 0, 0, -1.0F, 0.0F, -1.0F, 2, 2, 2, 0.0F));
		leftbackleg.cubeList.add(new ModelBox(leftbackleg, 0, 21, -1.0F, 1.0F, -2.0F, 2, 1, 1, 0.0F));
	}

	@Override
	public void render(Entity entity, float f, float f1, float f2, float f3, float f4, float f5) {
		body.render(f5);
		tail.render(f5);
		head.render(f5);
		rightfrontleg.render(f5);
		leftfrontleg.render(f5);
		rightbackleg.render(f5);
		leftbackleg.render(f5);
	}

	public void setRotationAngle(ModelRenderer modelRenderer, float x, float y, float z) {
		modelRenderer.rotateAngleX = x;
		modelRenderer.rotateAngleY = y;
		modelRenderer.rotateAngleZ = z;
	}
}
