 public class RenderBee
/*    */   extends RenderLiving
/*    */ {
	 /*    */   protected ModelBee model;
	 /* 15 */   private float scale = 1.0F;
	 /* 16 */   private static final ResourceLocation texture = new ResourceLocation(ChaosAwaken.MODID + ":textures/entity/bee.png");
	 /*    */   
		public RenderBee(ModelBase par1ModelBase, float par2) {
			super(par1ModelBase, par2);
		}
		
		protected ResourceLocation getEntityTexture(EntityBee entity) {
			return texture;
		}

		@Override
		protected ResourceLocation getEntityTexture(Entity entity) {
			return this.getEntityTexture((EntityBee)entity);
		}
