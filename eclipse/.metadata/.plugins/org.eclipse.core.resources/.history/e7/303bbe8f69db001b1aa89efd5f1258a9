package Me.Akio.ChaosAwakens.Renderers;

     import net.minecraft.client.model.ModelBase;
     import net.minecraft.client.model.ModelCow;
     import net.minecraft.client.renderer.entity.RenderLiving;
     import net.minecraft.entity.Entity;
     import net.minecraft.entity.EntityLiving;
     import net.minecraft.entity.EntityLivingBase;
     import net.minecraft.util.ResourceLocation;
     import org.lwjgl.opengl.GL11;


import Me.Akio.ChaosAwakens.Entities.Enchanted_Cow;
import Me.Akio.ChaosAwakens.Entities.Golden_Cow;
import Me.Akio.ChaosAwakens.Entities.Red_Cow;
     
     public class RenderEnchantedCow
       extends RenderLiving
     {

		public RenderEnchantedCow(ModelCow model, float shadowsize) {
			super(model, shadowsize);
			
		}
		
		protected ResourceLocation getEntityTexture(Entity P_110775_2_) {
			return new ResourceLocation("chaosawakens:textures/AppleCow");
		}
		
		public void doRender(Entity entity, double x, double z, float rotationView, float rotationPitch) {
			super.doRender(entity, rotationPitch, x, z, rotationView, rotationPitch);
		}

     }