package Me.Akio.ChaosAwakens.Models;

/*     */ import net.minecraft.client.model.ModelBase;
import net.minecraft.client.model.ModelBox;
/*     */ import net.minecraft.client.model.ModelRenderer;
/*     */ import net.minecraft.entity.Entity;
/*     */ import net.minecraft.util.MathHelper;
import net.minecraft.world.World;
/*     */ 
public class ModelBeaver extends ModelBase {
	/*  10 */   private float wingspeed = 1.0F;

	private final ModelRenderer body;
	private final ModelRenderer tail;
	private final ModelRenderer tail_r1;
	private final ModelRenderer head;
	private final ModelRenderer rightfrontleg;
	private final ModelRenderer leftfrontleg;
	private final ModelRenderer rightbackleg;
	private final ModelRenderer leftbackleg;

	public ModelBeaver() {
		textureWidth = 64;
		textureHeight = 32;

		this.body = new ModelRenderer(this);
		this.body.setRotationPoint(0.0F, 24.0F, 0.0F);
		this.body.cubeList.add(new ModelBox(body, 0, 0, -4.0F, -9.0F, -4.0F, 8, 7, 10, 0.0F));
		this.body.mirror = true;
		
		this.tail = new ModelRenderer(this);
		this.tail.setRotationPoint(0.0F, 21.0F, 5.0F);
		this.tail.mirror = true;

		this.tail_r1 = new ModelRenderer(this);
		this.tail_r1.setRotationPoint(0.0F, 3.0F, -5.0F);
		this.tail.addChild(tail_r1);
		this.setRotationAngle(tail_r1, 0.1309F, 0.0F, 0.0F);
		this.tail_r1.cubeList.add(new ModelBox(tail_r1, 0, 17, -3.0F, -3.0F, 6.0F, 6, 1, 8, 0.0F));
		this.tail_r1.mirror = true;

		this.head = new ModelRenderer(this);
		this.head.setRotationPoint(0.0F, 19.0F, -4.0F);
		this.head.cubeList.add(new ModelBox(head, 23, 21, -2.0F, -3.0F, -5.0F, 4, 4, 5, 0.0F));
		this.head.cubeList.add(new ModelBox(head, 20, 23, -1.0F, 1.0F, -5.0F, 2, 1, 1, 0.0F));
		this.head.mirror = true;

		this.rightfrontleg = new ModelRenderer(this);
		this.rightfrontleg.setRotationPoint(-2.0F, 22.0F, 0.0F);
		this.rightfrontleg.cubeList.add(new ModelBox(rightfrontleg, 0, 0, -1.0F, 0.0F, -3.0F, 2, 2, 2, 0.0F));
		this.rightfrontleg.cubeList.add(new ModelBox(rightfrontleg, 0, 21, -1.0F, 1.0F, -4.0F, 2, 1, 1, 0.0F));
		this.rightfrontleg.mirror = true;

		this.leftfrontleg = new ModelRenderer(this);
		this.leftfrontleg.setRotationPoint(2.0F, 22.0F, -2.0F);
		this.leftfrontleg.cubeList.add(new ModelBox(leftfrontleg, 0, 0, -1.0F, 0.0F, -1.0F, 2, 2, 2, 0.0F));
		this.leftfrontleg.cubeList.add(new ModelBox(leftfrontleg, 0, 21, -1.0F, 1.0F, -2.0F, 2, 1, 1, 0.0F));
		this.leftfrontleg.mirror = true;

		this.rightbackleg = new ModelRenderer(this);
		this.rightbackleg.setRotationPoint(-2.0F, 22.0F, 4.0F);
		this.rightbackleg.cubeList.add(new ModelBox(rightbackleg, 0, 0, -1.0F, 0.0F, -1.0F, 2, 2, 2, 0.0F));
		this.rightbackleg.cubeList.add(new ModelBox(rightbackleg, 0, 21, -1.0F, 1.0F, -2.0F, 2, 1, 1, 0.0F));
		this.rightbackleg.mirror = true;

		this.leftbackleg = new ModelRenderer(this);
		this.leftbackleg.setRotationPoint(2.0F, 22.0F, 4.0F);
		this.leftbackleg.cubeList.add(new ModelBox(leftbackleg, 0, 0, -1.0F, 0.0F, -1.0F, 2, 2, 2, 0.0F));
		this.leftbackleg.cubeList.add(new ModelBox(leftbackleg, 0, 21, -1.0F, 1.0F, -2.0F, 2, 1, 1, 0.0F));
		this.leftbackleg.mirror = true;

	}

	@Override
	public void render(Entity entity, float f, float f1, float f2, float f3, float f4, float f5) {
		
			/*  88 */     float newangle = 0.0F;
			/*  89 */     super.render(entity, f, f1, f2, f3, f4, f5);
			/*  90 */     setRotationAngles(f, f1, f2, f3, f4, f5, entity);
			/*     */ 
			/*     */     
			/*  93 */     newangle = MathHelper.cos(f2 * 1.7F * this.wingspeed) * 1.1415927F * 0.45F * f1;;
			/*  94 */     this.leftfrontleg.rotateAngleX = newangle;
			/*  95 */     this.rightfrontleg.rotateAngleX = -newangle;
						this.rightbackleg.rotateAngleX = -newangle;
						this.leftbackleg.rotateAngleX = -newangle;

			/*     */     
			/*     */     
			/* 100 */     newangle = MathHelper.cos(f2 * 0.5F * this.wingspeed) * 3.1415927F * 0.05F;
			/* 101 */     this.tail.rotateAngleX = newangle;
		
		this.body.render(f5);
		this.tail.render(f5);
		this.head.render(f5);
		this.rightfrontleg.render(f5);
		this.leftfrontleg.render(f5);
		this.rightbackleg.render(f5);
		this.leftbackleg.render(f5);
	}

	public void setRotationAngle(ModelRenderer modelRenderer, float x, float y, float z) {
		modelRenderer.rotateAngleX = x;
		modelRenderer.rotateAngleY = y;
		modelRenderer.rotateAngleZ = z;
	}
}
