  
package orespawnreforged.entities.renders;

import net.minecraft.client.model.ModelCow;
import net.minecraft.client.renderer.GlStateManager;
import net.minecraft.client.renderer.entity.Render;
import net.minecraft.client.renderer.entity.RenderLiving;
import net.minecraft.client.renderer.entity.RenderManager;
import net.minecraft.util.ResourceLocation;
import net.minecraftforge.fml.client.registry.IRenderFactory;
import net.minecraftforge.fml.client.registry.RenderingRegistry;
import net.minecraftforge.fml.relauncher.Side;
import net.minecraftforge.fml.relauncher.SideOnly;
import orespawnreforged.entities.EntityBee;
import orespawnreforged.entities.EntityEsquilo;
import orespawnreforged.entities.EntityMantis;
import orespawnreforged.entities.models.ModelBee;
import orespawnreforged.entities.models.ModelEsquilo;
import orespawnreforged.entities.models.ModelMantis;
import orespawnreforged.util.Reference;

@SideOnly(Side.CLIENT)
public class RenderBee extends RenderLiving<EntityBee>
{
	public static final ResourceLocation TEXTURE = new ResourceLocation(Reference.MODID + ":textures/entity/bee.png");
	
	public RenderBee(RenderManager manager) 
	{
		super(manager, new ModelBee(), 0.2f);
	}
	
	@Override
	protected ResourceLocation getEntityTexture(EntityBee entity) 
	{
		return TEXTURE;
	}
	
	public static void Render() {
		RenderingRegistry.registerEntityRenderingHandler(EntityBee.class, RenderBee::new);
	}
}